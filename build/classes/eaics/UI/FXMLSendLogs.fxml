<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eaics.UI.FXMLSendLogsController">
   <children>
      <Label alignment="CENTER" layoutX="359.0" layoutY="20.0" prefHeight="23.0" prefWidth="92.0" text="Logging">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <VBox alignment="CENTER" layoutX="55.0" layoutY="63.0" prefHeight="128.0" prefWidth="699.0" spacing="20.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="33.0" prefWidth="329.0" spacing="20.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="124.0" text="PC User and IP:" />
                  <TextField fx:id="ip" prefHeight="27.0" prefWidth="524.0" promptText="eg. pi@10.1.1.1" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="46.0" prefWidth="472.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="142.0" text="Path on local PC: " />
                  <TextField fx:id="path" prefHeight="27.0" prefWidth="525.0" promptText="eg. /home/pi/Desktop" text="~/Desktop/" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="70.0" prefWidth="591.0" spacing="50.0">
               <children>
                  <Button fx:id="closeButton" mnemonicParsing="false" onAction="#closeButtonAction" prefHeight="53.0" prefWidth="115.0" text="Exit" />
                  <Button mnemonicParsing="false" onAction="#sendLogsAction" prefHeight="53.0" prefWidth="174.0" text="Download Logs" />
                  <Button mnemonicParsing="false" onAction="#deleteLogsButtonAction" prefHeight="53.0" prefWidth="129.0" text="Delete Logs" />
               </children>
            </HBox>
         </children>
      </VBox>
      <VBox fx:id="root" layoutX="33.0" layoutY="213.0" prefHeight="249.0" prefWidth="734.0" />
   </children>
</AnchorPane>
